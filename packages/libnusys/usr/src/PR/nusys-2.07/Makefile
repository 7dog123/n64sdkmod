MAKE	=	make
CP		=	cp

default:
	$(MAKE) -C nusys      -f Makefile
	$(MAKE) -C nualsgi    -f Makefile
	$(MAKE) -C nualstl3    -f Makefile

install:
	$(MAKE) -C nusys      -f Makefile install 
	$(MAKE) -C nualsgi    -f Makefile install 
	$(MAKE) -C nualstl3    -f Makefile install 


clean:
	$(MAKE) -C nusys      -f Makefile clean 
	$(MAKE) -C nualsgi    -f Makefile clean
	$(MAKE) -C nualstl3    -f Makefile clean

