
#include "letters.h"

/*
 * ROM spec file
 */
beginseg
	name "code"
	flags BOOT OBJECT
	entry boot
	stack bootStack + STACKSIZE
	include "codesegment.o"
	include "/usr/lib/n64/PR/rspboot.o"
	include "/usr/lib/n64/PR/gspF3DEX2.xbus.o"
	include "/usr/lib/n64/PR/gspF3DEX2.fifo.o"
endseg

beginseg
        name "zbuffer"
        flags OBJECT
        address 0x80118000
        include "zbuffer.o"
endseg

beginseg
	name "cfb"
	flags OBJECT
	address 0x8013d800
	include "cfb.o"
endseg

beginseg
	name "static"
	flags OBJECT
	number STATIC_SEGMENT
	include "static.o"
endseg

beginwave
	name "letters"
	include "code"
	include "static"
	include "cfb"
	include "zbuffer"
endwave
